<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tv1_line_test"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:gravity="center"
        android:maxLength="3"
        android:maxLines="1"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv2_line_test"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:gravity="center"
        android:maxLength="8"
        android:maxLines="1"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/tv1_line_test"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv3_line_test"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:gravity="center"
        android:maxLength="9"
        android:maxLines="1"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/tv2_line_test"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv4_line_test"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="20dp"
        android:ellipsize="end"
        android:gravity="center"
        android:maxLength="11"
        android:maxLines="1"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv3_line_test"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/struct_view_8"
        android:layout_width="1dp"
        android:layout_height="0dp"
        android:layout_marginStart="5dp"
        android:background="#66000000"
        app:layout_constraintBottom_toBottomOf="@+id/tv1_line_test"
        app:layout_constraintStart_toEndOf="@+id/tv1_line_test"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/struct_view_9"
        android:layout_width="1dp"
        android:layout_height="0dp"
        android:layout_marginStart="5dp"
        android:background="#66000000"
        app:layout_constraintBottom_toBottomOf="@+id/tv1_line_test"
        app:layout_constraintStart_toEndOf="@+id/tv2_line_test"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/struct_view_10"
        android:layout_width="1dp"
        android:layout_height="0dp"
        android:layout_marginStart="5dp"
        android:background="#66000000"
        app:layout_constraintBottom_toBottomOf="@+id/tv1_line_test"
        app:layout_constraintStart_toEndOf="@+id/tv3_line_test"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/struct_view_11"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:background="#66000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv3_line_test" />

    <TextView
        android:id="@+id/struct_view_20"
        android:layout_width="1dp"
        android:layout_height="0dp"
        android:layout_marginStart="10dp"
        android:background="#66000000"
        app:layout_constraintBottom_toBottomOf="@+id/tv1_line_test"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/struct_view_21"
        android:layout_width="1dp"
        android:layout_height="0dp"
        android:layout_marginEnd="10dp"
        android:background="#66000000"
        app:layout_constraintBottom_toBottomOf="@+id/tv1_line_test"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>